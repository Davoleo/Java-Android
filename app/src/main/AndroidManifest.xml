<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="net.davoleo.java_android">

    <uses-permission android:name="android.permission.WRITE_CONTACTS"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".randomizer.Randomizer">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".geo_genius.GeoGeniusLogin"
                android:label="@string/title_activity_geo_genius">
        </activity>
        <activity
                android:name=".geo_genius.GeoGeniusHome"
                android:label="@string/title_activity_geo_genius_home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".geo_genius.sections.Capitals"
                android:label="@string/title_activity_capitals">
        </activity>
        <activity
                android:name=".geo_genius.sections.Rivers"
                android:label="@string/title_activity_rivers">
        </activity>
        <activity android:name=".geo_genius.ActivityContactList">
        </activity>
        <activity android:name=".movie_player.ActivityMoviePlayer">
        </activity>
    </application>

</manifest>